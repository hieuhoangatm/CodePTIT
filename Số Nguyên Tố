#include<stdio.h>
#include<math.h>
int main(){
	int a,n;
	scanf("%d", &n);
	while(n--){
	     scanf("%d", &a);
	     if(a>0){
	        int i=2;
	        int count=0;
	        for(;i<=sqrt(a);i++){
		         if(a%i==0)  count++;
			}
			if(count==0 && a>1) printf("YES\n", a);
			else printf("NO\n", a);
		}
	}
}
//Hoàng Đình Hiếu
