#include<stdio.h>
int main(){ 
    int a,b;
    scanf("%d%d", &a,&b);
    if(a>0 && a<b ){
    	int tong=0;
    	for(int i=a;i<=b;i++) tong+=i;
    	printf("%d", tong);
    }
    if(b>0 && b<a){
        int tong=0;
        for(int i=b;i<=a;i++){
           tong+=i;
        }
        printf("%d", tong);
    }
} 
// Hoàng Đình Hiếu 
