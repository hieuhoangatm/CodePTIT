#include<stdio.h>
int main(){
	long long t;
	scanf("%lld", &t);
	while(t--){
		long long N;
		int d1,d2;
		scanf("%lld", &N);
	    while(N>0){
	    	d1=N%10;
	    	N/=10;
	    	d2=N%10;
	    	if(d2>d1){
	    	   printf("NO\n");
	    	   break;
	    	}
	    	if(N==0){
	    	
	    	   printf("YES\n");
	    	   break;
	    	}
	    }
	}
}
