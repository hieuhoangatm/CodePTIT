#include<stdio.h>
#include<string.h>

int main(){
	int a;
	scanf("%d",&a);
	getchar();
	while(a--){
        char n[205];
        gets(n);
        int count =1;
        int i;
        for( i=0;i<strlen(n)-1;i++){
        	if(n[i] ==' ' && n[i+1] != ' ') count++;
        }
        printf("%d\n", count);
    }
}
        
