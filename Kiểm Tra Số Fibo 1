#include<stdio.h>
long long fb(long long n){
    if(n==1||n==0) return n;
   return fb(n-2)+fb(n-1);
}
int main()
{
   long long n,d=0;
   scanf("%lld",&n);
   for(long long i=0;i<=n;i++)
       if(n==fb(i))
       {
            d++;         
            break;
       }
   if(d==1) printf("1");
   else printf("0");
   
}
