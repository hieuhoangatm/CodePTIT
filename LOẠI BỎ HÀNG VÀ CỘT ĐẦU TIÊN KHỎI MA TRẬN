#include<stdio.h>

int main(){
	int t;
	scanf("%d",&t);
	for(int i=1;i<=t;i++){
        int n,m;
        scanf("%d%d",&n,&m);
        int a[n][m];
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++){
                scanf("%d",&a[i][j]);
            }
        }
        printf("Test %d:",i); 
        for(int i=0;i<n;i++){
            if(i!=0){
                for(int j=0;j<m;j++){
                    if(j!=0){
                        printf("%d ",a[i][j]);
                    }
                }
            }
        printf("\n");
        }
    }
}
