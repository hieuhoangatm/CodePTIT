#include<stdio.h>

void Nhap(int *n, int * arr){
	int i;
	scanf("%d", n);
	for(i=0;i<*n;i++){
		
		scanf("%d", &arr[i]);
	
	if(arr[i]%2==0)
	   printf("%d ", arr[i]);
	}
	printf("\n");
}

int main(){
	int k,i,arr[100];
	scanf("%d", &k);
	while(k>0){
		int n=0;
		Nhap(&n,arr);
		
		k--;
	}
    return 0;
}
